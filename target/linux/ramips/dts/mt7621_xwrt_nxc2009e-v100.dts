/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "xwrt,nxc2009e-v100", "mediatek,mt7621-soc";
	model = "NXC2009E-V100";

	aliases {
		led-status = &led_sys;
		label-mac-device = &gmac0;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		led_sys: sys {
			label = "green:sys";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		port0_M_led {
			label = "port0_M_led";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		port1_M_led {
			label = "port1_M_led";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		port2_M_led {
			label = "port2_M_led";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		port3_M_led {
			label = "port3_M_led";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&sdhci {
	status = "disabled";
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
			};

			partition@40000 {
				label = "product";
				reg = <0x40000 0x10000>;
			};

			factory: partition@50000 {
				label = "factory";
				reg = <0x50000 0x40000>;
			};

			partition@90000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x90000 0xf70000>;
			};
		};
	};
};

&ethernet {
	pinctrl-0 = <&rgmii1_pins &rgmii2_pins &mdio_pins>;
};

&gmac1 {
	status = "okay";
	phy-mode = "rgmii";
	label = "eth5";
	nvmem-cells = <&macaddr_factory_22>;
	nvmem-cell-names = "mac-address";
	mac-address-increment = <5>;

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&gmac0 {
	nvmem-cells = <&macaddr_factory_22>;
	nvmem-cell-names = "mac-address";
	label = "dsa";
};

&switch0 {
	ports {
		port@0 {
			status = "okay";
			label = "eth4";
			nvmem-cells = <&macaddr_factory_22>;
			nvmem-cell-names = "mac-address";
			mac-address-increment = <4>;
		};

		port@1 {
			status = "okay";
			label = "eth3";
			nvmem-cells = <&macaddr_factory_22>;
			nvmem-cell-names = "mac-address";
			mac-address-increment = <3>;
		};

		port@2 {
			status = "okay";
			label = "eth2";
			nvmem-cells = <&macaddr_factory_22>;
			nvmem-cell-names = "mac-address";
			mac-address-increment = <2>;
		};

		port@3 {
			status = "okay";
			label = "eth1";
			nvmem-cells = <&macaddr_factory_22>;
			nvmem-cell-names = "mac-address";
			mac-address-increment = <1>;
		};

		port@4 {
			status = "okay";
			label = "eth0";
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			groups = "wdt", "jtag";
			function = "gpio";
		};
	};
};

&factory {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_factory_22: macaddr@22 {
		reg = <0x22 0x6>;
	};
};
